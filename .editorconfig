# Editor configuration, see https://editorconfig.org
#tener instalado EditorConfig Support for VS Code: https://marketplace.visualstudio.com/items?itemName=EditorConfig.EditorConfig
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[*.js]
quote_type = single

[*.md]
max_line_length = off
trim_trailing_whitespace = false
